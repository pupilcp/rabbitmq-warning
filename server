global $argv;
if (!isset($argv[1]) || 'start' != $argv[1]) {
    exit('Invalid Operation [' . $argv[1] . ']');
}
require 'vendor/autoload.php';
$config = include 'config.php';

\LCP\GC::getInstance()->set($config);
$app = new \LCP\App($config);
$app->start();